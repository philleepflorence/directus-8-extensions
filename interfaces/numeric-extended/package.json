{
  "name": "numeric-extended",
  "version": "1.0.0",
  "description": "Extends the numeric interface to allow for min, max, and length",
  "main": "",
  "scripts": {
    "build": "directus-extensions build --input ./src --output ~/Sites/Directus/v8/suite/public/extensions/custom/interfaces/numeric-extended",
    "dev": "directus-extensions watch --input ./src --output ~/Sites/Directus/v8/suite/public/extensions/custom/interfaces/numeric-extended",
    "dist": "directus-extensions build --input ./src --output ./dist"
  },
  "keywords": [
    "numeric",
    "max",
    "min",
    "max-length"
  ],
  "author": "Philleep Florence",
  "license": "GPL-3.0",
  "dependencies": {
    "@directus/extension-toolkit": "^0.0.1",
    "vue": "^2.6.11",
    "vue-hot-reload-api": "^2.3.4"
  },
  "devDependencies": {
    "@vue/component-compiler-utils": "^2.6.0",
    "cssnano": "^4.1.10",
    "sass": "^1.26.10",
    "vue-template-compiler": "^2.6.11"
  }
}
