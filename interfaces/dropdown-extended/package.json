{
	"name": "dropdown-extended",
	"version": "1.3.0",
	"description": "Renders a dropdown interface that pulls values from a field in a collection",
	"main": "",
	"scripts": {
		"build": "directus-extensions build --input ./src --output ~/Sites/Directus/v8/suite/public/extensions/custom/interfaces/dropdown-extended",
		"dev": "directus-extensions watch --input ./src --output ~/Sites/Directus/v8/suite/public/extensions/custom/interfaces/dropdown-extended",
		"dist": "directus-extensions build --input ./src --output ./dist",
		"mixins": "rsync -rv ~/Sites/Directus/v8/extensions/mixins src && ls -al src/mixins"
	},
	"keywords": [
		"dropdown",
		"collection",
		"field",
		"dynamic"
	],
	"author": "Philleep Florence",
	"license": "GPL-3.0",
	"dependencies": {
		"@directus/extension-toolkit": "^0.0.1",
		"lodash": "^4.17.15",
		"vue": "^2.6.11",
		"vue-hot-reload-api": "^2.3.4"
	},
	"devDependencies": {
		"@vue/component-compiler-utils": "^2.6.0",
		"cssnano": "^4.1.10",
		"sass": "^1.17.3",
		"vue-template-compiler": "^2.6.10"
	}
}
