{
	"name": "Comments",
	"version": "1.3.0",
	"description": "Display all comments in the Directus Activity Collection",
	"main": "",
	"scripts": {
		"build": "directus-extensions build --input ./src --output ~/Sites/Directus/v8/suite/public/extensions/custom/modules/comments",
		"dev": "directus-extensions watch --input ./src --output ~/Sites/Directus/v8/suite/public/extensions/custom/modules/comments",
		"dist": "directus-extensions build --input ./src --output ./dist"
	},
	"keywords": [
		"custom",
		"modules",
		"comments"
	],
	"author": "Philleep Florence",
	"license": "GPL-3.0",
	"dependencies": {
		"@directus/extension-toolkit": "^0.0.1",
		"lodash": "^4.17.15",
		"vue": "^2.6.11",
		"vue-hot-reload-api": "^2.3.4",
		"vuex": "^3.1.2"
	},
	"devDependencies": {
		"@vue/component-compiler-utils": "^2.6.0",
		"cssnano": "^4.1.10",
		"sass": "^1.17.3",
		"vue-template-compiler": "^2.6.10"
	}
}
